package skt.tmall.cert.model.bean;

import java.io.Serializable;

import javax.xml.bind.annotation.XmlRootElement;


/**
 * 인증번호 상태조회 BO
 * @author mr_brain
 *
 */
@XmlRootElement(name = "SomCertApi")
public class SomCertApiBO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private String resultCode;			// 결과코
	private String resultMessage;		// 결과메시지
	private String certNo;				// 인증번호
	private String certStat;			// 인증상태
	private long ordNo;					// 주문번호
	private long ordPrdSeq;				// 주문상품순번
	private String certExprDt;			// 인증 만료일
	private String sellerPrdCd;			// 판매자 상품 코드

	public String getResultCode() {
		return resultCode;
	}
	public void setResultCode(String resultCode) {
		this.resultCode = resultCode;
	}
	public String getResultMessage() {
		return resultMessage;
	}
	public void setResultMessage(String resultMessage) {
		this.resultMessage = resultMessage;
	}
	public String getCertNo() {
		return certNo;
	}
	public void setCertNo(String certNo) {
		this.certNo = certNo;
	}
	public String getCertStat() {
		return certStat;
	}
	public void setCertStat(String certStat) {
		this.certStat = certStat;
	}	
	public long getOrdNo() {
		return ordNo;
	}
	public void setOrdNo(long ordNo) {
		this.ordNo = ordNo;
	}
	public long getOrdPrdSeq() {
		return ordPrdSeq;
	}
	public void setOrdPrdSeq(long ordPrdSeq) {
		this.ordPrdSeq = ordPrdSeq;
	}
	public String getCertExprDt() {
		return certExprDt;
	}
	public void setCertExprDt(String certExprDt) {
		this.certExprDt = certExprDt;
	}
	public String getSellerPrdCd() {
		return sellerPrdCd;
	}
	public void setSellerPrdCd(String sellerPrdCd) {
		this.sellerPrdCd = sellerPrdCd;
	}
}
