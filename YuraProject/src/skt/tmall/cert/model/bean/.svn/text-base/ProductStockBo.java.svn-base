package skt.tmall.cert.model.bean;

public class ProductStockBo {
	
	private long prdNo;		// 상품번호
	private long stckQty;		// 재고수량
	private long updateNo;	// 수정자
	
	public long getPrdNo() {
		return prdNo;
	}
	public void setPrdNo(long prdNo) {
		this.prdNo = prdNo;
	}
	public long getStckQty() {
		return stckQty;
	}
	public void setStckQty(long stckQty) {
		this.stckQty = stckQty;
	}
	public long getUpdateNo() {
		return updateNo;
	}
	public void setUpdateNo(long updateNo) {
		this.updateNo = updateNo;
	}

}
